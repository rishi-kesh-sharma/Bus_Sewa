
           <div class="row my-5">
            <div class="col">
              <table class="table bg-white rounded shadow-sm table-hover">
                <thead>
                  <tr>
                    <th scope="col" width="50">Name</th>
                    <th scope="col">Email</th>
                    <th scope="col">Phone</th>
                    <th scope="col">Address</th>
                    <th scope="col">Description</th>
                    <th scope="col">removeIcon</th>
                  </tr>
                </thead>
                <tbody>
                  
                  <% users.forEach((user,index)=>{  %>
                    
                    <tr>
                      <td><%= index+1 %> </td>
                      <td><%= user.name %></td>
                      <td><%= user.phone %></td>
                      <td><%= user.email %></td>
                      <td><%= user.address %></td>
                      <td><i class="fa fa-trash remove-btn" aria-hidden="true" id=<%=user._id%> ></i></td>
                    </tr>
                    
                  <% }) %>
 
                </tbody>
              </table> 
            </div>
          </div>